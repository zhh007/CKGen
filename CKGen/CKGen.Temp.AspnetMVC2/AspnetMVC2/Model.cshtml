@using RazorEngine.Templating
@using CKGen.Temp.AspnetMVC2
@using CKGen.DBSchema;
@inherits TemplateBase<PageViewModel>

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace @(Model.NameSpacePR).Data.Models
{
    public partial class @(Model.TableName)
    {
        @foreach (var column in Model.DBTable.Columns)
        {
        <text>/// @("<summary>")</text>
        <text>/// @(column.Description)</text>
        <text>/// @("</summary>")</text>
        <text>public @(column.LanguageType) @(column.PascalName) { get; set; }</text>
        @("\r\n")
        }
    }
}

